<p id="notice"><%= notice %></p>

<h1>Pub Art Has Students</h1>

<table>
  <thead>
    <tr>
      <th>Status</th>
      <th>Pub art</th>
      <th>Student</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pub_art_has_students.each do |pub_art_has_student| %>
      <tr>
        <td><%= pub_art_has_student.status %></td>
        <td><%= pub_art_has_student.pub_art %></td>
        <td><%= pub_art_has_student.student %></td>
        <td><%= link_to 'Show', pub_art_has_student %></td>
        <td><%= link_to 'Edit', edit_pub_art_has_student_path(pub_art_has_student) %></td>
        <td><%= link_to 'Destroy', pub_art_has_student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

