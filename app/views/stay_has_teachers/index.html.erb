<p id="notice"><%= notice %></p>

<h1>Stay Has Teachers</h1>

<table>
  <thead>
    <tr>
      <th>Status</th>
      <th>Stay</th>
      <th>Teacher</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stay_has_teachers.each do |stay_has_teacher| %>
      <tr>
        <td><%= stay_has_teacher.status %></td>
        <td><%= stay_has_teacher.stay %></td>
        <td><%= stay_has_teacher.teacher %></td>
        <td><%= link_to 'Show', stay_has_teacher %></td>
        <td><%= link_to 'Edit', edit_stay_has_teacher_path(stay_has_teacher) %></td>
        <td><%= link_to 'Destroy', stay_has_teacher, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stay Has Teacher', new_stay_has_teacher_path %>
