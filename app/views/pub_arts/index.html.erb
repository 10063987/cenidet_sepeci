<p id="notice"><%= notice %></p>

<h1>Pub Arts</h1>

<table>
  <thead>
    <tr>
      <th>Issn printed</th>
      <th>Issn electronic</th>
      <th>Magazine name</th>
      <th>Article title</th>
      <th>Grade</th>
      <th>Publication date</th>
      <th>Index</th>
      <th>Magazine number</th>
      <th>Magazine volume</th>
      <th>Year</th>
      <th>Start page</th>
      <th>End page</th>
      <th>Pub art type</th>
      <th>Evidence</th>
      <th>Acceptance letter</th>
      <th>Message</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pub_arts.each do |pub_art| %>
      <tr>
        <td><%= pub_art.issn_printed %></td>
        <td><%= pub_art.issn_electronic %></td>
        <td><%= pub_art.magazine_name %></td>
        <td><%= pub_art.article_title %></td>
        <td><%= pub_art.grade %></td>
        <td><%= pub_art.publication_date %></td>
        <td><%= pub_art.index %></td>
        <td><%= pub_art.magazine_number %></td>
        <td><%= pub_art.magazine_volume %></td>
        <td><%= pub_art.year %></td>
        <td><%= pub_art.start_page %></td>
        <td><%= pub_art.end_page %></td>
        <td><%= pub_art.pub_art_type %></td>
        <td><%= pub_art.evidence %></td>
        <td><%= pub_art.acceptance_letter %></td>
        <td><%= pub_art.message %></td>
        <td><%= link_to 'Show', pub_art %></td>
        <td><%= link_to 'Edit', edit_pub_art_path(pub_art) %></td>
        <td><%= link_to 'Destroy', pub_art, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pub Art', new_pub_art_path %>
