<p id="notice"><%= notice %></p>

<h1>Sw Dev Types</h1>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sw_dev_types.each do |sw_dev_type| %>
      <tr>
        <td><%= sw_dev_type.type %></td>
        <td><%= link_to 'Show', sw_dev_type %></td>
        <td><%= link_to 'Edit', edit_sw_dev_type_path(sw_dev_type) %></td>
        <td><%= link_to 'Destroy', sw_dev_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

