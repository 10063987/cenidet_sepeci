<p id="notice"><%= notice %></p>

<h1>Event Assistance Has Students</h1>

<table>
  <thead>
    <tr>
      <th>Status</th>
      <th>Event assistance</th>
      <th>Student</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_assistance_has_students.each do |event_assistance_has_student| %>
      <tr>
        <td><%= event_assistance_has_student.status %></td>
        <td><%= event_assistance_has_student.event_assistance %></td>
        <td><%= event_assistance_has_student.student %></td>
        <td><%= link_to 'Show', event_assistance_has_student %></td>
        <td><%= link_to 'Edit', edit_event_assistance_has_student_path(event_assistance_has_student) %></td>
        <td><%= link_to 'Destroy', event_assistance_has_student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

